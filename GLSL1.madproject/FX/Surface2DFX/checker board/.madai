            "code": "/*{\n  \"RESOURCE_TYPE\": \"Surface 2D FX For MadMapper\",\n  \"CREDIT\": \"Made with MadAI\",\n  \"DESCRIPTION\": \"Checker board pattern\",\n  \"TAGS\": \"pattern,geometric\",\n  \"VSN\": \"1.0\",\n  \"INPUTS\": [\n    { \"LABEL\": \"Size/Scale\", \"NAME\": \"fx_scale\", \"TYPE\": \"float\", \"DEFAULT\": 10.0, \"MIN\": 1.0, \"MAX\": 50.0 },\n    { \"LABEL\": \"Size/Ratio\", \"NAME\": \"fx_ratio\", \"TYPE\": \"float\", \"DEFAULT\": 1.0, \"MIN\": 0.1, \"MAX\": 10.0 },\n    { \"LABEL\": \"Offset/X\", \"NAME\": \"fx_offset_x\", \"TYPE\": \"float\", \"DEFAULT\": 0.0, \"MIN\": -1.0, \"MAX\": 1.0 },\n    { \"LABEL\": \"Offset/Y\", \"NAME\": \"fx_offset_y\", \"TYPE\": \"float\", \"DEFAULT\": 0.0, \"MIN\": -1.0, \"MAX\": 1.0 }\n  ]\n}*/\n\nvec2 f_adjustUV(vec2 p_uv) {\n    float v_aspectRatio = FX_IMG_SIZE().x / FX_IMG_SIZE().y;\n    p_uv.x *= v_aspectRatio;\n    return p_uv;\n}\n\nfloat f_checker(vec2 p_uv) {\n    p_uv = f_adjustUV(p_uv);\n    p_uv *= fx_scale;\n    p_uv.x *= fx_ratio;\n    p_uv += vec2(fx_offset_x, fx_offset_y) * fx_scale;\n    vec2 v_grid = floor(p_uv);\n    return mod(v_grid.x + v_grid.y, 2.0);\n}\n\nvec4 fxColorForPixel(vec2 mm_FragNormCoord) {\n    vec4 v_color = FX_NORM_PIXEL(mm_FragNormCoord);\n    float v_pattern = f_checker(mm_FragNormCoord);\n    return v_color * v_pattern;\n}",
