  }\n        v_hue /= 6.0;\n    }\n    \n    return vec3(v_hue, v_saturation, v_value);\n}\n\nvec3 f_hsv_to_rgb(vec3 p_hsv) {\n    float v_hue = p_hsv.x * 6.0;\n    float v_saturation = p_hsv.y;\n    float v_value = p_hsv.z;\n    \n    float v_c = v_value * v_saturation;\n    float v_x = v_c * (1.0 - abs(mod(v_hue, 2.0) - 1.0));\n    float v_m = v_value - v_c;\n    \n    vec3 v_rgb = vec3(0.0);\n    \n    if (v_hue < 1.0) {\n        v_rgb = vec3(v_c, v_x, 0.0);\n    } else if (v_hue < 2.0) {\n        v_rgb = vec3(v_x, v_c, 0.0);\n    } else if (v_hue < 3.0) {\n        v_rgb = vec3(0.0, v_c, v_x);\n    } else if (v_hue < 4.0) {\n        v_rgb = vec3(0.0, v_x, v_c);\n    } else if (v_hue < 5.0) {\n        v_rgb = vec3(v_x, 0.0, v_c);\n    } else {\n        v_rgb = vec3(v_c, 0.0, v_x);\n    }\n    \n    return v_rgb + v_m;\n}\n\nvec4 fxColorForPixel(vec2 mm_FragNormCoord) {\n    vec4 v_input_color = FX_NORM_PIXEL(mm_FragNormCoord);\n    \n    // Apply brightness\n    vec3 v_bright_color = v_input_color.rgb + fx_brightness;\n    \n    // Apply contrast\n    vec3 v_contrast_color = (v_bright_color - 0.5) * fx_contrast + 0.5;\n    \n    // Apply desaturation\n    float v_luminance = dot(v_contrast_color, vec3(0.299, 0.587, 0.114));\n    vec3 v_gray = vec3(v_luminance);\n    vec3 v_desaturated = mix(v_contrast_color, v_gray, fx_desaturate);\n    \n    // Convert to HSV for hue manipulation\n    vec3 v_hsv = f_rgb_to_hsv(v_desaturated);\n    \n    // Apply hue shifts based on RGB channels\n    vec3 v_hue_shift = vec3(fx_hue_red, fx_hue_green, fx_hue_blue);\n    float v_total_shift = (v_hue_shift.r + v_hue_shift.g + v_hue_shift.b) * 0.333;\n    v_hsv.x = mod(v_hsv.x + v_total_shift, 1.0);\n    \n    // Convert back to RGB\n    vec3 v_final_color = f_hsv_to_rgb(v_hsv);\n    \n    // Apply individual RGB channel adjustments\n    v_final_color.r = clamp(v_final_color.r + fx_hue_red * 0.2, 0.0, 1.0);\n    v_final_color.g = clamp(v_final_color.g + fx_hue_green * 0.2, 0.0, 1.0);\n    v_final_color.b = clamp(v_final_color.b + fx_hue_blue * 0.2, 0.0, 1.0);\n    \n    return vec4(v_final_color, v_input_color.a);\n}",
